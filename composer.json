{
    "name": "dilum/puppy-data-table",
    "description": "Lightweight Laravel DataTable with Yajra-like features and a vanilla JS frontend",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dilum\\PuppyDataTable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dilum\\PuppyDataTable\\PuppyDataTableServiceProvider"
            ],
            "aliases": {
                "PuppyDataTable": "Dilum\\PuppyDataTable\\Facades\\PuppyDataTable"
            }
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0"
    }
}